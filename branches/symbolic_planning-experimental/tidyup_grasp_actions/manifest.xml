<package>
  <description brief="tidyup_grasp_actions">
    This package contains state and action definitions for the
    continual_planning_executive that interface the tidyup grasping application.
  </description>
  <author>Christian Dornhege</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/tidyup_grasp_actions</url>
  <depend package="roscpp"/>
  <depend package="continual_planning_executive"/>
  <depend package="pluginlib"/>
  <depend package="actionlib"/>
  <depend package="tidyup_msgs"/>
  <depend package="tf"/>
  <depend package="angles"/>
  <depend package="hardcoded_facts"/>
  <depend package="pr2_common_action_msgs"/>
  <depend package="visualization_msgs"/>

  <export>
      <continual_planning_executive plugin="${prefix}/tidyup_grasp_actions_plugins.xml" />
  </export>

</package>


