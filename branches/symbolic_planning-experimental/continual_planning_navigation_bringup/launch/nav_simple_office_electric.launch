<launch>
  <!-- start up world -->
  <include file="$(find gazebo_worlds)/launch/office_world.launch"/>

  <!-- start up robot -->
  <include file="$(find pr2_gazebo)/pr2.launch"/>

  <!-- load map -->
  <node pkg="map_server" type="map_server" args="$(find continual_planning_navigation_bringup)/launch/simple_office.yaml" respawn="true" name="map1" />

  <!-- Tuck Arms For Navigation -->
  <!-- <node pkg="pr2_tuck_arms_action" type="tuck_arms.py" args="-q -l t -r t" output="screen" name="tuck_arms" /> Fixme: Already started by pr2_2dnav -->

  <!-- nav-stack -->
  <include file="$(find pr2_2dnav)/pr2_2dnav.launch" />

  <!-- for visualization -->
  <include file="$(find pr2_navigation_global)/rviz/rviz_move_base.launch"/>

</launch>
